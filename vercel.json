{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/images(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/api/properties(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/api/users(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
