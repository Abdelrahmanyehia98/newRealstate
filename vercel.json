{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/routes/images(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/routes/properties(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/routes/users(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
